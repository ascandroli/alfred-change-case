				<string>#!/bin/bash

SWIFT_FILE="changecase.swift"
EXECUTABLE="./changecase"
HASH_FILE=".changecase.md5"

CURRENT_HASH=$(md5 -q "$SWIFT_FILE")

STORED_HASH=$(cat "$HASH_FILE" 2&gt;/dev/null)

if [ "$CURRENT_HASH" != "$STORED_HASH" ] || [ ! -f "$EXECUTABLE" ]; then
    swiftc "$SWIFT_FILE" -o "$EXECUTABLE" || exit 1
fi</string>
